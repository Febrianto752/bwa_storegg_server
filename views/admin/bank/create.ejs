<%- include("./../../partials/header") %> <%- include("./../../partials/navbar")
%> <%- include("./../../partials/sidebar") %>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>Bank</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="/">Home</a></li>
            <li class="breadcrumb-item"><a href="/bank">Bank</a></li>
            <li class="breadcrumb-item active">Tambah</li>
          </ol>
        </div>
      </div>
    </div>
    <!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-header">
              <h3 class="card-title"></h3>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <div class="row">
                <div class="col-12">
                  <!-- general form elements -->
                  <div class="card card-primary">
                    <div class="card-header">
                      <h3 class="card-title">Form Tambah Bank</h3>
                    </div>
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form action="/bank/create" method="post">
                      <div class="card-body">
                        <div class="form-group">
                          <label for="owner">Masukan Pemilik Rekening</label>
                          <input
                            type="text"
                            class="form-control"
                            id="owner"
                            name="owner"
                            placeholder="Masukan nama pemilik rekening..."
                          />
                        </div>

                        <div class="form-group">
                          <label for="bankName">Masukan Nama Bank</label>
                          <input
                            type="text"
                            class="form-control"
                            id="bankName"
                            name="bankName"
                            placeholder="Masukan nama bank"
                          />
                        </div>

                        <div class="form-group">
                          <label for="noRekening">Masukan Rekening</label>
                          <input
                            type="text"
                            class="form-control"
                            id="noRekening"
                            name="noRekening"
                            placeholder="Masukan nomor rekening..."
                          />
                        </div>
                      </div>
                      <!-- /.card-body -->

                      <div class="card-footer">
                        <button type="submit" class="btn btn-primary">
                          Submit
                        </button>
                      </div>
                    </form>
                  </div>
                  <!-- /.card -->

                  <!-- /.card -->
                </div>
              </div>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </section>
  <!-- /.content -->
</div>

<%- include("./../../partials/footer") %> <%- include("./../../partials/js") %>
